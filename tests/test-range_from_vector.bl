def plusColumnReduce(A){
    #_;
    return (+%(A^T))^T;
}


def rangeFromVector(v){
    #logical;
    vlogic = v~1;
    #arithmetic;
    n = plusColumnReduce(v);
    u = Zero(1:n);
    j = 0;
    i = 0;
    B = |v|[0];
    while ( i < B ) {
        if (v[i]) {
            u[j] = i;
            j = j + 1;
        }
        i = i + 1;
        B = |v|[0];
    }
    return u;
}

A = rangeFromVector([3;0;1;2;0;5]);
print(toString(A));
