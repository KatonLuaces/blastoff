def plusColumnReduce(A){
    #_;
    return (+%(A^T))^T;
}


def rangeFromVector(v){
    #logical;
    vlogic = v~1;
    #arithmetic;
    n = plusColumnReduce(vlogic);
    u = Zero(n:1);
    j = 0;
    i = 0;
    m = |v|[0];
    while ( i < m) {
        if (v[i]) {
            u[j] = i;
            j = j + 1;
        }
        i = i + 1;
    }
    return u;
}

A = rangeFromVector([3;0;1;2;0;5]);
print(toString(A));
