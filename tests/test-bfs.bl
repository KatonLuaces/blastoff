def One(size){
    A = Zero(size);
    oneSize = |A|;
    A[range(oneSize[0]), range(oneSize[1])] = 1;
    return A;
}

size = 3;
print(toString(One(size)));
def BFS(G, frontier){
    #logical;
    N = |G|[0];
    levels = Zero(N, 1);
    maskedGT = G^T;
    depth = 0;
    while (sum(frontier)) {
        #arithmetic;
        depth = depth + 1;
        #logical;
        levels[rangeFromVector(frontier)] = depth;
        mask = !(frontier^T)[Zeroes(N), 0, 1, N];
        maskedGT = maskedGT @ mask;
        frontier = maskedGT*frontier;
    }
    #arithmetic;
    return levels + (One(|levels|)~(-1));
}

G = [0,0,1; 1,0,0; 0,1,0];
frontier = [1,0,0];
BFS(G, frontier);
